{
    "version": 2,
    "builds": [
      {
        "src": "/index.py",
        "use": "@vercel/python",
        "config": {
        "buildCommand": "pip install --upgrade pip && pip install --no-cache-dir -r requirements.txt"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.py"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }
  